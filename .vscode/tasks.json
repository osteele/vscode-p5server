{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "watch",
      "dependsOn": [
        "link dependencies"
      ],
      "problemMatcher": "$tsc-watch",
      "isBackground": true,
      "presentation": {
        "reveal": "never"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "link dependencies",
      "type": "npm",
      "script": "p5-server:link",
      "problemMatcher": []
    },
    {
      "label": "unlink dependencies",
      "type": "npm",
      "script": "p5-server:unlink",
      "problemMatcher": []
    }
  ]
}
