{
  "name": "p5-server",
  "displayName": "p5-server",
  "description": "Launch a p5js-aware local server",
  "version": "0.0.1",
  "publisher": "osteele",
  "author": {
    "name": "Oliver Steele",
    "email": "steele@osteele.com"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "live reload",
    "p5",
    "p5.js"
  ],
  "activationEvents": [
    "onCommand:p5-server.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "p5-server.start",
        "title": "Start the P5 server"
      },
      {
        "command": "p5-server.stop",
        "title": "Stop the P5 server"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "p5-server": "^0.1.3"
  },
  "devDependencies": {
    "@types/vscode": "^1.58.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
